    {{>header}}
  <form action="/signup" method="post">
    <div class="container rounded bg-white mt-5 mb-5 px-2">
        <div class="row ctr">
            <div class="col-md-5 border-right">
                <div class="p-3 py-4">
                    <div class="col-md-36 border-right">
                        <div class="d-flex flex-column align-items-center text-center p-3 py-1"><img
                                class="rounded-circle mt-0" width="150px"
                                src="https://st3.depositphotos.com/15648834/17930/v/600/depositphotos_179308454-stock-illustration-unknown-person-silhouette-glasses-profile.jpg"><span
                                class="font-weight-bold">Create Profile</span>
                            <div class="d-flex justify-content-center">
                                <div class="btn btn-outline-info">
                                    <label class="form-label text-black m-0" for="customFile1">Add Image</label>
                                    <input type="file" class="form-control d-none" id="customFile1" />
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row mt-2">
                        <div class="col-md-6"><label class="labels">Name</label><input type="text" class="form-control"
                                placeholder="First name" value="" name="firstname"></div>
                        <div class="col-md-6"><label class="labels">Last Name</label><input type="text"
                                class="form-control" value="" placeholder="Last Name" name="lastname"></div>
                    </div>

                    <div>
                        <div class="col-md-6 mt-3"><label class="labels">Gender</label></div>
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="gender" id="inlineRadio1"
                                value="male">
                            <label class="form-check-label" for="inlineRadio1">Male</label>
                        </div>
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="gender" id="inlineRadio2"
                                value="female">
                            <label class="form-check-label" for="inlineRadio2">Female</label>
                        </div>
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="gender" id="inlineRadio2"
                                value="others">
                            <label class="form-check-label" for="inlineRadio2">Others</label>
                        </div>
                    </div>


                    <div>
                        <div class="col-md-6 mt-3"><label class="labels">Batch</label></div>
                            <input type="number" name="batch" id="" min="1940" max="2030" class="form-control" value="2020">
                        </select>
                    </div>

                    <div>
                        <div class="col-md-6 mt-3"><label class="labels">College</label></div>
                        <select class="form-select form-select-sm form-control" aria-label=".form-select-sm example" name="college">
                            <option selected>Select College</option>
                            <option value="Indian Institute of Information Technology, Surat">Indian Institute of Information Technology, Surat</option>
                            <option value="Indian Institute of Information Technology, Kota">Indian Institute of Information Technology, Kota</option>
                            <option value="Indian Institute of Information Technology, Pune">Indian Institute of Information Technology, Pune</option>
                            <option value="Indian Institute of Information Technology, Kancheepuram">Indian Institute of Information Technology, Kancheepuram</option>
                            <option value="Indian Institute of Information Technology, Jabalpur">Indian Institute of Information Technology, Jabalpur</option>
                            <option value="Indian Institute of Information Technology, Vadodara">Indian Institute of Information Technology, Vadodara</option>
                            <option value="Indian Institute of Information Technology, Delhi">Indian Institute of Information Technology, Delhi</option>
                        </select>
                    </div>
                    <div>
                        <div class="col-md-6 mt-3"><label class="labels">Branch</label></div>
                        <select class="form-select form-select-sm form-control" aria-label=".form-select-sm example" name="branch">
                            <option selected>Select Branch</option>
                            <option value="CSE">CSE</option>
                            <option value="IT">IT</option>
                            <option value="ECE">ECE</option>
                            <option value="EE">EE</option>
                            <option value="ME">ME</option>
                            <option value="CE">CE</option>
                        </select>
                    </div>
                    <div class="row mt-3">
                        <div class="col-md-6"><label class="labels address">City</label><input type="text"
                                class="form-control" placeholder="Enter Your City" value="" name="city"></div>
                        <div class="col-md-6"><label class="label address">State</label><input type="text"
                                class="form-control" value="" placeholder="Enter Your State" name="state"></div>
                    </div>
                    <div class="col-md-12 mt-3"><label class="labels">Contact Number</label><input type="number"
                            class="form-control" placeholder="Enter Contact Number" value="" name="contact"></div>

                    <div class="col-md-12 mt-3"><label class="labels">Email ID</label><input type="email"
                            class="form-control" placeholder="Enter email id" value="" name="email"></div>

                    <div class="col-md-12 mt-3"><label class="labels">LinkedIn handle</label><input type="url"
                            class="form-control" placeholder="Enter LinkedIn handle" value="" name="linkedin"></div>
                    <div class="col-md-12 mt-3"><label class="labels">Instagram handle</label><input type="url"
                            class="form-control" placeholder="Enter Instagram handle" value="" name="instagram"></div>

                    <div class="col-md-12 mt-3"><label class="labels Work">Current Designation</label><input type="text"
                            class="form-control" placeholder="Designation" value="" name="designation"></div>


                    <div class="col-md-12">
                        <div class="mt-3">
                            <div class="col-md-12"><label class="labels">Skills & Experience</label>
                                <textarea name="skills" id="skills" cols="30" rows="4"
                                    class="form-control" name="skills"></textarea>
                            </div> <br>


                            <div class="col-md-12 mt-1"><label class="labels">About</label>
                                <textarea name="about" id="skills" cols="30" rows="3"
                                    class="form-control" name="about"></textarea>
                            </div> <br>
                            
                            <div class="row mt-2">
                                <div class="col-md-6"><label class="labels">Password</label><input type="text" class="form-control"
                                        placeholder="Password" value="" name="password"></div>
                                <div class="col-md-6"><label class="labels">Confirm Password</label><input type="password"
                                        class="form-control" value="" placeholder="Confirm Password" name="cpassword"></div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="mt-0 mb-2 text-center">
                        <input type="submit" value="save profile">
                        </div>
            </div>
        </div>
    </div>
    </div>
    </div>
    </div>
</form>
</body>
</html>
    {{>header}}
    <!-- <section style="background-color: #eee;"> -->
    <section style="background-color: #d2a475;">
      <div class="container py-4 px-2">
        

        <div class="row">
          <div class="col-lg-4">
            <div class="card mb-4">
              <div class="card-body text-center cardCSS">
                <img
                  src="\Profile-Avatar-PNG.png"
                  alt=""
                  class="rounded-circle img-fluid"
                  style="width: 150px;"
                />
                
                <h5 class="my-3">{{firstname}} {{lastname}}</h5>
                <p class="text-muted mb-1 my-10">{{designation}}</p>
                <!-- <p class="text-muted mb-4">Bay Area, San Francisco, CA</p> -->
                <div class="d-flex justify-content-center mb-2 my-4">
                  <button type="button" class="btn btn-primary">Follow</button>
                  <button
                    type="button"
                    class="btn btn-outline-primary ms-1"
                  >Message</button>
                </div>
              </div>
            </div>

            <div class="card mb-4 mb-lg-0 cardCSS">
              <div class="card-body p-0">
                <ul class="list-group list-group-flush rounded-3">
                  <div class="card-body">
                    <div class="row">
                      <div class="col-sm-3">
                        <p class="mb-0">Email</p>
                      </div>
                      <div class="col-sm-9">
                        <p class="text-muted mb-0">{{email}}</p>
                      </div>
                    </div>
                    <hr />
                    <div class="row">
                      <div class="col-sm-3">
                        <p class="mb-0">Contact</p>
                      </div>
                      <div class="col-sm-9">
                        <p class="text-muted mb-0">{{contact}}</p>
                      </div>
                    </div>
                    <hr />

                    <div class="row">
                      <div class="col-sm-3">
                        <p class="mb-0">Linkedin</p>
                      </div>
                      <div class="col-sm-9">
                        <a
                          href="{{linkedin}}"
                          target="_blank"
                        >{{firstname}} {{lastname}}</a>
                      </div>
                    </div>
                    <hr />

                    <div class="row">
                      <div class="col-sm-3">
                        <p class="mb-0">Instagram</p>
                      </div>
                      <div class="col-sm-9">
                        <a
                          href="{{instagram}}"
                          target="_blank"
                        >{{firstname}} {{lastname}}</a>
                      </div>
                    </div>
                  </div>
                </ul>
              </div>
            </div>
          </div>

          <div class="col-lg-8">
            <div class="card mb-4 cardCSS">
              <div class="card-body">
                <div class="row">
                  <div class="col-sm-3">
                    <p class="mb-0">Full Name</p>
                  </div>
                  <div class="col-sm-9">
                    <p class="text-muted mb-0">{{firstname}} {{lastname}}</p>
                  </div>
                </div>
                <hr />
                <div class="row">
                  <div class="col-sm-3">
                    <p class="mb-0">Gender</p>
                  </div>
                  <div class="col-sm-9">
                    <p class="text-muted mb-0">{{gender}}</p>
                  </div>
                </div>
                <hr />
                <div class="row">
                  <div class="col-sm-3">
                    <p class="mb-0"> College </p>
                  </div>
                  <div class="col-sm-9">
                    <p class="text-muted mb-0">{{college}}</p>
                  </div>
                </div>
                <hr />
                <div class="row">
                  <div class="col-sm-3">
                    <p class="mb-0">Batch</p>
                  </div>
                  <div class="col-sm-9">
                    <p class="text-muted mb-0">{{batch}}</p>
                  </div>
                </div>
                <hr />
                <div class="row">
                  <div class="col-sm-3">
                    <p class="mb-0">Address</p>
                  </div>
                  <div class="col-sm-9">
                    <p class="text-muted mb-0">{{city}},{{state}}</p>
                  </div>
                </div>
              </div>
            </div>
            <div class="row pb-4">
              <div class="col-md-12">
                <div class="card mb-4 mb-md-0">
                  <div class="card-body cardCSS">
                    <p class="mb-4"><span
                        class="text-primary font-italic me-1"
                      >Skills And Experience
                      </span>
                    </p>
                    <ul class="list-group list-group-flush cardCSS">
                      <li class="list-group-item cardCSS">{{skills}}</li>
                    </ul>
                  </div>
                </div>
              </div>
            </div>
            <div class="row">
              <div class="col-md-12">
                <div class="card mb-4 mb-md-0">
                  <div class="card-body cardCSS">
                    <p class="mb-4"><span
                        class="text-primary font-italic me-1"
                      >About
                      </span>
                    </p>
                    <ul class="list-group list-group-flush cardCSS">
                      <li class="list-group-item cardCSS">{{about}}</li>
                    </ul>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>

  </body>

</html>